{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environmentName": {
      "value": "${AZURE_ENV_NAME}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "connectorType": {
      "value": "${CONNECTOR_TYPE}"
    },
    "amazonBedrockAccessKeyId": {
      "value": "${AMAZON_BEDROCK_ACCESS_KEY_ID}"
    },
    "amazonBedrockSecretAccessKey": {
      "value": "${AMAZON_BEDROCK_SECRET_ACCESS_KEY}"
    },
    "amazonBedrockRegion": {
      "value": "${AMAZON_BEDROCK_REGION}"
    },
    "amazonBedrockModelId": {
      "value": "${AMAZON_BEDROCK_MODEL_ID=amazon.nova-micro-v1:0}"
    },
    "azureAIFoundryEndpoint": {
      "value": "${AZURE_AI_FOUNDRY_ENDPOINT}"
    },
    "azureAIFoundryApiKey": {
      "value": "${AZURE_AI_FOUNDRY_API_KEY}"
    },
    "azureAIFoundryDeploymentName": {
      "value": "${AZURE_AI_FOUNDRY_DEPLOYMENT_NAME=gpt-4o-mini}"
    },
    "githubModelsToken": {
      "value": "${GH_MODELS_TOKEN}"
    },
    "githubModelsModel": {
      "value": "${GH_MODELS_MODEL=openai/gpt-4o-mini}"
    },
    "huggingFaceModel": {
      "value": "${HUGGING_FACE_MODEL=hf.co/Qwen/Qwen3-0.6B-GGUF}"
    },
    "lgModel": {
      "value": "${LG_MODEL=hf.co/LGAI-EXAONE/EXAONE-4.0-1.2B-GGUF}"
    },
    "openAIModel": {
      "value": "${OPENAI_MODEL=gpt-4.1-mini}"
    },
    "openAIApiKey": {
      "value": "${OPENAI_API_KEY}"
    },
    "upstageModel": {
      "value": "${UPSTAGE_MODEL}"
    },
    "upstageBaseUrl": {
      "value": "${UPSTAGE_BASE_URL}"
    },
    "upstageApiKey": {
      "value": "${UPSTAGE_API_KEY}"
    },
    "gpuProfileName": {
      "value": "${GPU_PROFILE_NAME=NC8as-T4}"
    },
    "openchatPlaygroundAppExists": {
      "value": "${SERVICE_OPENCHAT_PLAYGROUNDAPP_RESOURCE_EXISTS=false}"
    },
    "principalId": {
      "value": "${AZURE_PRINCIPAL_ID}"
    },
    "principalType": {
      "value": "${AZURE_PRINCIPAL_TYPE}"
    }
  }
}
