{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environmentName": {
      "value": "${AZURE_ENV_NAME}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "connectorType": {
      "value": "${CONNECTOR_TYPE}"
    },
    "githubModelsModel": {
      "value": "${GH_MODELS_MODEL=openai/gpt-4o-mini}"
    },
    "githubModelsToken": {
      "value": "${GH_MODELS_TOKEN}"
    },
    "huggingFaceModel": {
      "value": "${HUGGING_FACE_MODEL=hf.co/Qwen/Qwen3-0.6B-GGUF}"
    },
    "ollamaModel": {
        "value": "${OLLAMA_MODEL}"
    },
    "ollamaBaseUrl": {
        "value": "${OLLAMA_BASE_URL}"
    },
    "openAIModel": {
      "value": "${OPENAI_MODEL=gpt-4.1-mini}"
    },
    "openAIApiKey": {
      "value": "${OPENAI_API_KEY}"
    },
    "gpuProfileName": {
      "value": "${GPU_PROFILE_NAME=NC8as-T4}"
    },
    "openchatPlaygroundAppExists": {
      "value": "${SERVICE_OPENCHAT_PLAYGROUNDAPP_RESOURCE_EXISTS=false}"
    },
    "principalId": {
      "value": "${AZURE_PRINCIPAL_ID}"
    },
    "principalType": {
      "value": "${AZURE_PRINCIPAL_TYPE}"
    }
  }
}
